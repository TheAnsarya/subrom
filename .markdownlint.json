{
	"$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
	"default": true,
	"MD003": {
		"style": "atx"
	},
	"MD004": {
		"style": "dash"
	},
	"MD007": {
		"indent": 4
	},
	"MD009": {
		"br_spaces": 2,
		"strict": false
	},
	"MD010": false,
	"MD012": {
		"maximum": 2
	},
	"MD013": false,
	"MD022": false,
	"MD024": {
		"siblings_only": true
	},
	"MD025": {
		"front_matter_title": ""
	},
	"MD026": {
		"punctuation": ".,;:!"
	},
	"MD029": {
		"style": "ordered"
	},
	"MD031": false,
	"MD032": true,
	"MD033": {
		"allowed_elements": [
			"details",
			"summary",
			"kbd",
			"br",
			"sup",
			"sub",
			"img"
		]
	},
	"MD035": {
		"style": "---"
	},
	"MD036": false,
	"MD040": true,
	"MD041": true,
	"MD046": {
		"style": "fenced"
	},
	"MD048": {
		"style": "backtick"
	},
	"MD049": {
		"style": "underscore"
	},
	"MD050": {
		"style": "asterisk"
	},
	"MD058": true,
	"MD060": false
}
